{
	"image": "mcr.microsoft.com/devcontainers/universal:2",
	"waitFor": "onCreateCommand",
	"updateContentCommand": "npm install",
	"postCreateCommand": "docker pull ghcr.io/ayaka-notes/texlive-full:2023.1",
	"postAttachCommand": {
	  "server": "npm start"
	},
	"portsAttributes": {
	  "3000": {
		"label": "Application",
		"onAutoForward": "openPreview"
	  }
	},
	"forwardPorts": [3000]
  }