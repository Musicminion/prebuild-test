{
	"name": "overleaf",
	"dockerComposeFile": "docker-compose.yml",
	"service": "common",
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {
			"version": "latest",
			"enableNonRootDocker": "true",
			"moby": "true"
		},
		"ghcr.io/devcontainers/features/sshd:1": {
			"version": "latest"
		},
		"github-cli": "latest"
	},
	// 端口映射
	"forwardPorts": [80],
}